entity Expense {
  expenseType String required,
  amount Double required min(0),
  description String maxlength(100),
  date LocalDate
}

enum ExpenseType {
  FIXED
  VARIABLE
  NECESSARY
  DISCRETIONARY
  ONE_TIME
  SAVINGS_AND_INVESTMENT
  DEBT_REPAYMENT
  OTHER
}

relationship ManyToOne {
  Expenses {user(login)} to User
}



